  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program: check_run.js",
      "program": "${workspaceFolder}/lib/src/events/check_run.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program: index.js",
      "program": "${workspaceFolder}/lib/src/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Probot",
      "program": "${workspaceFolder}/node_modules/probot/bin/probot.js",
      "args": ["run", "./lib/src/index.js"]
    }
  ]
}
